    },
    {
      "source": "http://www.fenixdevacademy.com.br/(.*)",
      "destination": "https://fenixdevacademy.com.br/$1",
      "permanent": true
    },
    {
      "source": "https://www.fenixdevacademy.com.br/(.*)",
      "destination": "https://fenixdevacademy.com.br/$1",
      "permanent": true
    }
  ]
}