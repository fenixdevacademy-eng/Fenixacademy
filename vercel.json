{
  "version": 2,
  "name": "fenix-academy",
  "alias": [
    "fenixdevacademy.com.br",
    "www.fenixdevacademy.com.br"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "redirects": [
    {
      "source": "http://fenixdevacademy.com.br/(.*)",
